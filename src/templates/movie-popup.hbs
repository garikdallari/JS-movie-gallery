<div class="modal_movie">
    
    <div class="loader">
        <div class="item-1"></div>
        <div class="item-2"></div>
        <div class="item-3"></div>
        <div class="item-4"></div>
        <div class="item-5"></div>
     </div>

    <div class="thumb_image">
        {{#if data.poster_path}}
        <img src='https://image.tmdb.org/t/p/w500{{data.poster_path}}' alt="{{data.title}}" class="thumb_image_image" />
        {{else}}
        <img src='data:image/png;base64,' alt="{{data.title}}" class="thumb_image_image" />
        {{/if}}
    </div>

    <div class="modal_movie_info">
        <h2 class="modal_movie_title">{{data.title}}</h2>
        <div class="list">
            <div class="list_wrapper"><span class="list_name vote">Vote / Votes</span><span class="list_text"><span
                        class="list_text_vote">{{data.vote_average}}</span><span class="list_text_vote_count">/</span>
                    {{data.vote_count}}</span></div>
            <div class="list_wrapper"><span class="list_name popularity">Popularity</span><span class="list_text">
                    {{data.popularity}}</span></div>
            <div class="list_wrapper"><span class="list_name title">Original Title</span><span
                    class="list_text list_text--bigger">{{data.title}}</span></div>
            <div class="list_wrapper"><span class="list_name genre">Genre</span>
        
        <span class="list_text">{{genres}}</span>
    
        </div>       
        </div>
       

<div class="backdrop "> 
     <div class="backdrop_overlay"></div>
<div class="plyr__video-embed" id="player" style="display: none;">
 <iframe class="ytplayer"
        src="https://www.youtube.com/embed/{{key}}?origin=https://plyr.io&amp;iv_load_policy=3&amp;modestbranding=1&amp;playsinline=1&amp;showinfo=0&amp;rel=0&amp;enablejsapi=1"
        allowfullscreen allowtransparency allow="autoplay">
</iframe>
    <button type="button" class="trailer_button" data-action="close-trailer">
    
        <svg data-action="close-modal" height="30px" fill="#ff6b01">
    
            <path
                d="M26.667 7.303l-1.97-1.97-7.808 7.808-7.808-7.808-1.97 1.97 7.808 7.808-7.808 7.808 1.97 1.97 7.808-7.808 7.808 7.808 1.97-1.97-7.808-7.808 7.808-7.808z">
            </path>
    
        </svg>
    
    </button>
    </div>
</div>

    </div>
<div class="blur_back">
    <div class="blur_back__wrapper">
        <img class="blur_back__img"
            src='https://image.tmdb.org/t/p/original{{data.backdrop_path}}'
            alt="" />
            </div>
    </div>
</div>
<div class="modal_movie_about-film">
            <h3 class="title_about">ABOUT</h3>
            <p class="about_text"> {{data.overview}}</p>
            <div class="button_wrapper" data-value="modal-btns">
                <button class="button_watched watched" data-action="add-to-watched" data-id="{{data.id}}">add to watched</button>
                <button class="button_queue queue" data-action="add-to-queue" data-id="{{data.id}}">add to queue</button>
                <button class="button_open">watch trailer</button>
            </div>
</div>
</div>
